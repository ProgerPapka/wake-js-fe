{
    "extends": [
        "tslint-immutable"
    ],
    "rules": {
        "no-var-keyword": true,
        "no-parameter-reassignment": true,
        "typedef": false,
        "readonly-keyword": false,
        "readonly-array": false,
        "no-let": false,
        "radix": true,
        "switch-default": true,
        "triple-equals": [
            true,
            "allow-undefined-check",
            "allow-null-check"
        ],
        "eofline": false,
        "max-line-length": [
            true,
            120
        ],
        "no-trailing-whitespace": true,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "jsdoc-format": true,
        "no-consecutive-blank-lines": [
            true
        ],
        "semicolon": [
            true,
            "always"
        ]
    }
}
